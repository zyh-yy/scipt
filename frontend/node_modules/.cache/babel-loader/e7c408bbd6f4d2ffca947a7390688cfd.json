emplate>\r\n  <div class=\"scripts\">\r\n    <div class=\"page-container\">\r\n      <div class=\"page-header\">\r\n        <h1 class=\"page-title\">脚本管理</h1>\r\n        <div class=\"button-container\">\r\n          <el-button type=\"primary\" @click=\"$router.push('/scripts/add')\">\r\n            <i class=\"el-icon-plus\"></i> 添加脚本\r\n          </el-button>\r\n        </div>\r\n      </div>\r\n\r\n      <el-table\r\n        v-loading=\"loading\"\r\n        :data=\"scripts\"\r\n        border\r\n        style=\"width: 100%\"\r\n        empty-text=\"暂无数据\"\r\n      >\r\n        <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"脚本名称\" min-width=\"150\"></el-table-column>\r\n        <el-table-column prop=\"description\" label=\"描述\" min-width=\"200\"></el-table-column>\r\n        <el-table-column prop=\"file_type\" label=\"类型\" width=\"100\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag :type=\"getFileTypeTag(scope.row.file_type)\">\r\n              {{ scope.row.file_type }}\r\n            </el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"created_at\" label=\"创建时间\" width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            {{ formatTime(scope.row.created_at) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"250\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"handleView(scope.row)\"\r\n            >\r\n              查看\r\n            </el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"success\"\r\n              @click=\"handleExecute(scope.row)\"\r\n            >\r\n              执行\r\n            </el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"warning\"\r\n              @click=\"handleEdit(scope.row)\"\r\n            >\r\n              编辑\r\n            </el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.row)\"\r\n            >\r\n              删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n\r\n    <!-- 执行脚本对话框 -->\r\n    <el-dialog title=\"执行脚本\" :visible.sync=\"executeDialogVisible\" width=\"50%\">\r\n      <div v-if=\"currentScript && currentScript.parameters && currentScript.parameters.length\">\r\n        <el-form ref=\"executeForm\" :model=\"executeParams\" label-width=\"100px\">\r\n          <el-form-item \r\n            v-for=\"param in currentScript.parameters\" \r\n            :key=\"param.id\"\r\n            :label=\"param.name\"\r\n            :prop=\"param.name\"\r\n            :required=\"param.is_required === 1\"\r\n          >\r\n            <el-input \r\n              v-if=\"param.param_type === 'string'\" \r\n              v-model=\"executeParams[param.name]\"\r\n              :placeholder=\"param.description\"\r\n            ></el-input>\r\n            <el-input-number \r\n              v-else-if=\"param.param_type === 'number'\" \r\n              v-model=\"executeParams[param.name]\"\r\n              :placeholder=\"param.description\"\r\n            ></el-input-number>\r\n            <el-select\r\n              v-else-if=\"param.param_type === 'select'\"\r\n              v-model=\"executeParams[param.name]\"\r\n              :placeholder=\"param.description\"\r\n            >\r\n              <el-option\r\n                v-for=\"(option, idx) in param.options\"\r\n                :key=\"idx\"\r\n                :label=\"option\"\r\n                :value=\"option\"\r\n              ></el-option>\r\n            </el-select>\r\n            <el-switch\r\n              v-else-if=\"param.param_type === 'boolean'\"\r\n              v-model=\"executeParams[param.name]\"\r\n            ></el-switch>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n      <div v-else>\r\n        <p>该脚本没有需要配置的参数</p>\r\n      </div>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"executeDialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"confirmExecute\" :loading=\"executing\">执行</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <!-- 执行结果对话框 -->\r\n    <el-dialog title=\"执行结果\" :visible.sync=\"resultDialogVisible\" width=\"60%\">\r\n      <div v-loading=\"executing\">\r\n        <el-alert\r\n          v-if=\"executeResult\"\r\n          :title=\"executeResult.success ? '执行成功' : '执行失败'\"\r\n          :type=\"executeResult.success ? 'success' : 'error'\"\r\n          :description=\"executeResult.message\"\r\n          show-icon\r\n          :closable=\"false\"\r\n        ></el-alert>\r\n        \r\n        <div v-if=\"executeResult && executeResult.output\" class=\"result-output\">\r\n          <h3>输出:</h3>\r\n          <pre>{{ executeResult.output }}</pre>\r\n        </div>\r\n        \r\n        <div v-if=\"executeResult && executeResult.error\" class=\"result-error\">\r\n          <h3>错误:</h3>\r\n          <pre>{{ executeResult.error }}</pre>\r\n        </div>\r\n      </div>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"resultDialogVisible = false\">关闭</el-button>\r\n        <el-button \r\n          type=\"primary\" \r\n          @click=\"viewExecutionHistory\"\r\n          v-if=\"executeResult && executeResult.history_id\"\r\n        >\r\n          查看执行记录\r\n        </el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex';\r\n\r\nexport default {\r\n  name: 'Scripts',\r\n  data() {\r\n    return {\r\n      executeDialogVisible: false,\r\n      resultDialogVisible: false,\r\n      currentScript: null,\r\n      executeParams: {},\r\n      executing: false,\r\n      executeResult: null\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(['scripts', 'loading'])\r\n  },\r\n  created() {\r\n    this.fetchData();\r\n  },\r\n  methods: {\r\n    fetchData() {\r\n      this.$store.dispatch('fetchScripts');\r\n    },\r\n    formatTime(time) {\r\n      if (!time) return '-';\r\n      const date = new Date(time);\r\n      return date.toLocaleString();\r\n    },\r\n    getFileTypeTag(type) {\r\n      switch (type) {\r\n        case 'py':\r\n          return 'primary';\r\n        case 'sh':\r\n          return 'success';\r\n        case 'bat':\r\n          return 'warning';\r\n        case 'ps1':\r\n          return 'danger';\r\n        case 'js':\r\n          return 'info';\r\n        default:\r\n          return '';\r\n      }\r\n    },\r\n    handleView(row) {\r\n      this.$router.push(`/scripts/${row.id}`);\r\n    },\r\n    handleEdit(row) {\r\n      this.$router.push(`/scripts/${row.id}/edit`);\r\n    },\r\n    handleExecute(row) {\r\n      this.currentScript = row;\r\n      this.executeParams = {};\r\n      \r\n      // 初始化参数默认值\r\n      if (row.parameters) {\r\n        row.parameters.forEach(param => {\r\n          if (param.default_value) {\r\n            this.executeParams[param.name] = param.default_value;\r\n          }\r\n        });\r\n      }\r\n      \r\n      this.executeDialogVisible = true;\r\n    },\r\n    handleDelete(row) {\r\n      this.$confirm('此操作将永久删除该脚本, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$axios.delete(`/api/scripts/${row.id}`)\r\n          .then(response => {\r\n            if (response.data.code === 0) {\r\n              this.$message.success('删除成功');\r\n              this.fetchData();\r\n            } else {\r\n              this.$message.error(response.data.message || '删除失败');\r\n            }\r\n          })\r\n          .catch(error => {\r\n            this.$message.error('删除失败: ' + error.message);\r\n          });\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消删除'\r\n        });          \r\n      });\r\n    },\r\n    confirmExecute() {\r\n      this.executing = true;\r\n      this.executeDialogVisible = false;\r\n      this.resultDialogVisible = true;\r\n      this.executeResult = null;\r\n      \r\n      this.$store.dispatch('executeScript', {\r\n        scriptId: this.currentScript.id,\r\n        params: this.executeParams\r\n      }).then(result => {\r\n        this.executeResult = {\r\n          success: result.code === 0,\r\n          message: result.message,\r\n          output: result.data && result.data.output,\r\n          error: result.data && result.data.error,\r\n          history_id: result.data && result.data.history_id\r\n        };\r\n      }).catch(error => {\r\n        this.executeResult = {\r\n          success: false,\r\n          message: '执行请求失败',\r\n          error: error.message\r\n        };\r\n      }).finally(() => {\r\n        this.executing = false;\r\n      });\r\n    },\r\n    viewExecutionHistory() {\r\n      if (this.executeResult && this.executeResult.history_id) {\r\n        this.$router.push(`/history/${this.executeResult.history_id}`);\r\n        this.resultDialogVisible = false;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.page-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.result-output, .result-error {\r\n  margin-top: 20px;\r\n  \r\n  h3 {\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  pre {\r\n    background-color: #f5f7fa;\r\n    border: 1px solid #e6e6e6;\r\n    border-radius: 4px;\r\n    padding: 15px;\r\n    overflow-x: auto;\r\n    white-space: pre-wrap;\r\n    word-wrap: break-word;\r\n    max-height: 300px;\r\n    overflow-y: auto;\r\n  }\r\n}\r\n\r\n.result-error pre {\r\n  background-color: #fef0f0;\r\n  border-color: #fbc4c4;\r\n}\r\n</style>\r\n"],"mappings":"AA2JA,SAAAA,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,oBAAA;MACAC,mBAAA;MACAC,aAAA;MACAC,aAAA;MACAC,SAAA;MACAC,aAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAT,QAAA;EACA;EACAU,QAAA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACAD,UAAA;MACA,KAAAE,MAAA,CAAAC,QAAA;IACA;IACAC,WAAAC,IAAA;MACA,KAAAA,IAAA;MACA,MAAAC,IAAA,OAAAC,IAAA,CAAAF,IAAA;MACA,OAAAC,IAAA,CAAAE,cAAA;IACA;IACAC,eAAAC,IAAA;MACA,QAAAA,IAAA;QACA;UACA;QACA;UACA;QACA;UACA;QACA;UACA;QACA;UACA;QACA;UACA;MACA;IACA;IACAC,WAAAC,GAAA;MACA,KAAAC,OAAA,CAAAC,IAAA,aAAAF,GAAA,CAAAG,EAAA;IACA;IACAC,WAAAJ,GAAA;MACA,KAAAC,OAAA,CAAAC,IAAA,aAAAF,GAAA,CAAAG,EAAA;IACA;IACAE,cAAAL,GAAA;MACA,KAAAlB,aAAA,GAAAkB,GAAA;MACA,KAAAjB,aAAA;;MAEA;MACA,IAAAiB,GAAA,CAAAM,UAAA;QACAN,GAAA,CAAAM,UAAA,CAAAC,OAAA,CAAAC,KAAA;UACA,IAAAA,KAAA,CAAAC,aAAA;YACA,KAAA1B,aAAA,CAAAyB,KAAA,CAAA9B,IAAA,IAAA8B,KAAA,CAAAC,aAAA;UACA;QACA;MACA;MAEA,KAAA7B,oBAAA;IACA;IACA8B,aAAAV,GAAA;MACA,KAAAW,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAf,IAAA;MACA,GAAAgB,IAAA;QACA,KAAAC,MAAA,CAAAC,MAAA,iBAAAhB,GAAA,CAAAG,EAAA,IACAW,IAAA,CAAAG,QAAA;UACA,IAAAA,QAAA,CAAAtC,IAAA,CAAAuC,IAAA;YACA,KAAAC,QAAA,CAAAC,OAAA;YACA,KAAAhC,SAAA;UACA;YACA,KAAA+B,QAAA,CAAAE,KAAA,CAAAJ,QAAA,CAAAtC,IAAA,CAAA2C,OAAA;UACA;QACA,GACAC,KAAA,CAAAF,KAAA;UACA,KAAAF,QAAA,CAAAE,KAAA,YAAAA,KAAA,CAAAC,OAAA;QACA;MACA,GAAAC,KAAA;QACA,KAAAJ,QAAA;UACArB,IAAA;UACAwB,OAAA;QACA;MACA;IACA;IACAE,eAAA;MACA,KAAAxC,SAAA;MACA,KAAAJ,oBAAA;MACA,KAAAC,mBAAA;MACA,KAAAI,aAAA;MAEA,KAAAK,MAAA,CAAAC,QAAA;QACAkC,QAAA,OAAA3C,aAAA,CAAAqB,EAAA;QACAuB,MAAA,OAAA3C;MACA,GAAA+B,IAAA,CAAAa,MAAA;QACA,KAAA1C,aAAA;UACAmC,OAAA,EAAAO,MAAA,CAAAT,IAAA;UACAI,OAAA,EAAAK,MAAA,CAAAL,OAAA;UACAM,MAAA,EAAAD,MAAA,CAAAhD,IAAA,IAAAgD,MAAA,CAAAhD,IAAA,CAAAiD,MAAA;UACAP,KAAA,EAAAM,MAAA,CAAAhD,IAAA,IAAAgD,MAAA,CAAAhD,IAAA,CAAA0C,KAAA;UACAQ,UAAA,EAAAF,MAAA,CAAAhD,IAAA,IAAAgD,MAAA,CAAAhD,IAAA,CAAAkD;QACA;MACA,GAAAN,KAAA,CAAAF,KAAA;QACA,KAAApC,aAAA;UACAmC,OAAA;UACAE,OAAA;UACAD,