器\r\naxios.interceptors.response.use(\r\n  response => {\r\n    // 如果返回的状态码为200，说明接口请求成功，可以正常拿到数据\r\n    if (response.status === 200) {\r\n      return Promise.resolve(response);\r\n    } else {\r\n      return Promise.reject(response);\r\n    }\r\n  },\r\n  error => {\r\n    if (error.response && error.response.status) {\r\n      switch (error.response.status) {\r\n        case 401:\r\n          // 401: 未登录\r\n          // 跳转登录页\r\n          router.replace({\r\n            path: '/login',\r\n            query: { redirect: router.currentRoute.fullPath }\r\n          });\r\n          break;\r\n        case 403:\r\n          // 403: token过期\r\n          // 跳转登录页\r\n          router.replace({\r\n            path: '/login',\r\n            query: { redirect: router.currentRoute.fullPath }\r\n          });\r\n          break;\r\n        case 404:\r\n          // 404请求不存在\r\n          Vue.prototype.$message.error('请求的资源不存在');\r\n          break;\r\n        default:\r\n          Vue.prototype.$message.error('网络异常，请稍后再试');\r\n      }\r\n      return Promise.reject(error.response);\r\n    }\r\n    return Promise.reject(error);\r\n  }\r\n);\r\n\r\nVue.prototype.$axios = axios;\r\n\r\n// 配置ElementUI\r\nVue.use(ElementUI, { size: 'small' });\r\n\r\nVue.config.productionTip = false;\r\n\r\nnew Vue({\r\n  router,\r\n  store,\r\n  render: h => h(App)\r\n}).$mount('#app');\r\n"],"mappings":"AAAA,OAAOA,GAAG,MAAM,KAAK;AACrB,OAAOC,SAAS,MAAM,YAAY;AAClC,OAAO,sCAAsC;AAC7C,OAAOC,GAAG,MAAM,WAAW;AAC3B,OAAOC,MAAM,MAAM,UAAU;AAC7B,OAAOC,KAAK,MAAM,SAAS;AAC3B,OAAOC,KAAK,MAAM,OAAO;;AAEzB;AACAA,KAAK,CAACC,QAAQ,CAACC,OAAO,GAAGC,OAAO,CAACC,GAAG,CAACC,eAAe,IAAI,uBAAuB;AAC/EL,KAAK,CAACC,QAAQ,CAACK,OAAO,GAAG,KAAK;;AAE9B;AACAN,KAAK,CAACO,YAAY,CAACC,OAAO,CAACC,GAAG,CAC5BC,MAAM,IAAI;EACR,OAAOA,MAAM;AACf,CAAC,EACDC,KAAK,IAAI;EACP,OAAOC,OAAO,CAACC,MAAM,CAACF,KAAK,CAAC;AAC9B,CACF,CAAC;;AAED;AACAX,KAAK,CAACO,YAAY,CAACO,QAAQ,CAACL,GAAG,CAC7BK,QAAQ,IAAI;EACV;EACA,IAAIA,QAAQ,CAACC,MAAM,KAAK,GAAG,EAAE;IAC3B,OAAOH,