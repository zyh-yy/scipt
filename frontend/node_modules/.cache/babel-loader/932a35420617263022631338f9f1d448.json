n      } catch (error) {\r\n        commit('setError', error.message);\r\n        throw error;\r\n      } finally {\r\n        commit('setLoading', false);\r\n      }\r\n    },\r\n    \r\n    // 执行脚本链\r\n    async executeChain({ commit }, { chainId, params }) {\r\n      commit('setLoading', true);\r\n      commit('setError', null);\r\n      try {\r\n        const response = await axios.post(`/api/execution/chain/${chainId}`, params);\r\n        return response.data;\r\n      } catch (error) {\r\n        commit('setError', error.message);\r\n        throw error;\r\n      } finally {\r\n        commit('setLoading', false);\r\n      }\r\n    }\r\n  },\r\n  getters: {\r\n    scriptById: state => id => {\r\n      return state.scripts.find(script => script.id === id) || null;\r\n    },\r\n    chainById: state => id => {\r\n      return state.chains.find(chain => chain.id === id) || null;\r\n    },\r\n    historyById: state => id => {\r\n      return state.histories.find(history => history.id === id) || null;\r\n    }\r\n  }\r\n});\r\n"],"mappings":"AAAA,OAAOA,GAAG,MAAM,KAAK;AACrB,OAAOC,IAAI,MAAM,MAAM;AACvB,OAAOC,KAAK,MAAM,OAAO;AAEzBF,GAAG,CAACG,GAAG,CAACF,IAAI,CAAC;AAEb,eAAe,IAAIA,IAAI,CAACG,KAAK,CAAC;EAC5BC,KAAK,EAAE;IACLC,OAAO,EAAE,EAAE;IACXC,MAAM,EAAE,EAAE;IACVC,SAAS,EAAE,EAAE;IACbC,OAAO,EAAE,KAAK;IACdC,KAAK,EAAE;EACT,CAAC;EACDC,SAAS,EAAE;IACTC,UAAUA,CAACP,KAAK,EAAEC,OAAO,EAAE;MACzBD,KAAK,CAACC,OAAO,GAAGA,OAAO;IACzB,CAAC;IACDO,SAASA,CAACR,KAAK,EAAEE,MAAM,EAAE;MACvBF,KAAK,CAACE,MAAM,GAAGA,MAAM;IACvB,CAAC;IACDO,YAAYA,CAACT,KAAK,EAAEG,SAAS,EAAE;MAC7BH,KAAK,CAACG,SAAS,GAAGA,SAAS;IAC7B,CAAC;IACDO,UAAUA,CAACV,KAAK,EAAEI,OAAO,EAAE;MACzBJ,KAAK,CAACI,OAAO,GAAGA,OAAO;IACzB,CAAC;IACDO,QAAQA,CAACX,KAAK,EAAEK,KAAK,EAAE;MACrBL,KAAK,CAACK,KAAK,GAAGA,KAAK;IACrB;EACF,CAAC;EACDO,OAAO,EAAE;IACP;IACA,MAAMC,YAAYA,CAAC;MAAEC;IAAO,CAAC,EAAE;MAC7BA,MAAM,CAAC,YAAY,EAAE,IAAI,CAAC;MAC1BA,MAAM,CAAC,UAAU,EAAE,IAAI,CAAC;MACxB,IAAI;QACF,MAAMC,QAAQ,GAAG,MAAMlB,KAAK,CAACmB,GAAG,CAAC,cAAc,CAAC;QAChD,IA